<% @network_options.each do |key,value| %>
<%=key%>=<%=value%>
<% end %>
<% if @virt_type == "qemu" -%>
libvirt_use_virtio_for_bridges=false
<% else -%>
libvirt_use_virtio_for_bridges=true
<% end -%>
<% if not @vpn_image_id.nil? %>
vpn_image_id=<%= @vpn_image_id %>
<% end %>
dnsmasq_config_file=<%= @dnsmasq_config_file %>
